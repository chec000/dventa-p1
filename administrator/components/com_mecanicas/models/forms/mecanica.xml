<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset name="myfields">
    <field name="id" type="text" default="0" label="JGLOBAL_FIELD_ID_LABEL" readonly="true" class="readonly" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="state" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox small" size="1" default="1" >
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="usergroup" type="usergroup" label="JFIELD_ACCESS_LABEL" description="JFIELD_ACCESS_DESC" size="1" />
    <field name="publish_down" type="calendar" class="inputbox" size="22" format="%Y-%m-%d %H:%M:%S" default="NOW" label="COM_DEALERS_FIELD_PUBLISHED_LABEL" description="COM_DEALERS_FIELD_PUBLISHED_DESC" filter="user_utc" required="true" />
    <field name="content" type="editor" label="Contenido" description="" width="300" filter="safehtml" />
  </fieldset>
</form>
