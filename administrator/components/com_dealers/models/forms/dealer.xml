<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset name="myfields">
    <field name="id" type="text" default="0" label="JGLOBAL_FIELD_ID_LABEL" readonly="true" class="readonly" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="cedis_id" type="text" class="inputbox" size="10" label="COM_DEALERS_FIELD_CEDISID_LABEL" description="COM_DEALERS_FIELD_CEDISID_DESC" required="true" />
    <field name="names_cedis" type="text" class="inputbox" size="60" maxlength="255" label="COM_DEALERS_FIELD_NAMESCEDIS_LABEL" description="COM_DEALERS_FIELD_NAMESCEDIS_DESC" required="true" />
    <field name="street" type="text" class="inputbox" size="60" maxlength="255"  label="COM_DEALERS_FIELD_STREET_LABEL" description="COM_DEALERS_FIELD_STREET_DESC" required="true" />
    <field name="ext_number" type="text" class="inputbox" size="10" label="COM_DEALERS_FIELD_EXTNUMBER_LABEL" description="COM_DEALERS_FIELD_EXTNUMBER_DESC" required="true" />
    <field name="int_number" type="text" class="inputbox" size="10" label="COM_DEALERS_FIELD_INTNUMBER_LABEL" description="COM_DEALERS_FIELD_INTNUMBER_DESC" />
    <field name="location" type="text" class="inputbox" size="60" label="COM_DEALERS_FIELD_LOCATION_LABEL" description="COM_DEALERS_FIELD_LOCATION_DESC" />
    <field name="reference" type="text" class="inputbox" size="60" maxlength="255" label="COM_DEALERS_FIELD_REFERENCE_LABEL" description="COM_DEALERS_FIELD_REFERENCE_DESC" />
    <field name="estate" type="text" class="inputbox" size="60" label="COM_DEALERS_FIELD_ESTATE_LABEL" description="COM_DEALERS_FIELD_ESTATE_DESC" required="true" />
    <field name="city" type="text" class="inputbox" size="60" label="COM_DEALERS_FIELD_CITY_LABEL" description="COM_DEALERS_FIELD_CITY_DESC" required="true" />
    <field name="zip_code" type="text" class="inputbox" size="10" label="COM_DEALERS_FIELD_ZIPCODE_LABEL" description="COM_DEALERS_FIELD_ZIPCODE_DESC" required="true" />
    <field name="telephone" type="text" class="inputbox" size="18" label="COM_DEALERS_FIELD_TELEPHONE_LABEL" description="COM_DEALERS_FIELD_TELEPHONE_DESC" required="true" />
    <field name="state" type="list" label="COM_DEALERS_FIELD_STATUS_LABEL" description="COM_DEALERS_FIELD_STATUS_DESC" class="inputbox small" size="1" default="1" >
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="publish_down" type="calendar" class="inputbox" size="22" format="%Y-%m-%d %H:%M:%S" default="NOW" label="COM_DEALERS_FIELD_PUBLISHED_LABEL" description="COM_DEALERS_FIELD_PUBLISHED_DESC" filter="user_utc" required="true" />
  </fieldset>
</form>
